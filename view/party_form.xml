<data>
    <xpath
        expr="/form/notebook/page[@id='general']/field[@name='addresses']"
        position="after">
        <label name="commercial_name"/>
        <field name="commercial_name"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='accounting']/label[@name='vat_number']"
        position="before">
        <label name="type_document"/>
        <field name="type_document"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='accounting']/field[@name='vat_number']"
        position="after">
        <label name="check_digit"/>
        <field name="check_digit"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='accounting']"
        position="after">
        <page string="Fiscal" id="fiscal">
            <label name="mandatory_accounting"/>
            <field name="mandatory_accounting"/>
            <label name="vat_number_city"/>
            <field name="vat_number_city"/>
            <label name="first_name"/>
            <field name="first_name"/>
            <label name="second_name"/>
            <field name="second_name"/>
            <label name="first_family_name"/>
            <field name="first_family_name"/>
            <label name="second_family_name"/>
            <field name="second_family_name"/>
        </page>
    </xpath>
</data>
